{
  "name": "larpit-fi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma migrate dev && prisma generate --no-hints && next dev --turbopack --port 3158",
    "build": "prisma generate --no-hints && next build",
    "start": "next start",
    "lint": "prisma generate --no-hints && next lint",
    "email:dev": "npx email dev -d src/emails -p 3159",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "db:migrate": "prisma migrate deploy",
    "k8s:generate": "node --experimental-strip-types src/bin/manifest.mts",
    "k8s:secret": "node --experimental-strip-types src/bin/secret.mts",
    "k8s:dev": "npm run k8s:generate && npm run k8s:secret && skaffold dev"
  },
  "dependencies": {
    "@js-temporal/polyfill": "^0.5.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.13.0",
    "@react-email/components": "^0.5.0",
    "@react-email/render": "^1.2.0",
    "bootstrap": "^5.3.7",
    "isomorphic-dompurify": "^2.26.0",
    "jsdom": "^27.0.1",
    "marked": "^16.1.2",
    "next": "^15.4.6",
    "next-auth": "^4.24.13",
    "next-intl": "^4.3.4",
    "nodemailer": "^7.0.10",
    "p-limit": "^7.0.0",
    "prisma": "^6.18.0",
    "react": "19.1.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "19.1.0",
    "underscore": "^1.13.7",
    "uuid": "^13.0.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@react-email/preview-server": "^4.2.8",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.9.1",
    "@types/nodemailer": "^7.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/underscore": "^1.13.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "rdflib": "^2.2.37",
    "react-email": "^4.2.8",
    "sass": "^1.90.0",
    "tsx": "^4.20.3",
    "typescript": "^5"
  },
